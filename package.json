{
  "name": "react-19-telephone",
  "description": "☎️ Tiniest react input phone component (auto formating included) - React 19 support",
  "version": "1.0.1",
  "type": "module",
  "source": "./src/index.tsx",
  "main": "./dist/react-19-telephone.cjs",
  "module": "./dist/react-19-telephone.module.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/react-19-telephone.module.js",
      "umd": "./dist/react-19-telephone.umd.js",
      "import": "./dist/react-19-telephone.mjs",
      "require": "./dist/react-19-telephone.cjs"
    }
  },
  "scripts": {
    "dev": "microbundle watch",
    "build": "microbundle --jsx 'React.createElement' --jsxImportSource react --globals react/jsx-runtime=jsx",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx .",
    "tsc": "tsc --noEmit"
  },
  "license": "MIT",
  "homepage": "https://github.com/tvhampton/react-19-telephone#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tvhampton/react-19-telephone.git"
  },
  "bugs": {
    "url": "https://github.com/tvhampton/react-19-telephone/issues"
  },
  "keywords": [
    "phone",
    "phone number",
    "telephone",
    "international",
    "tel",
    "number"
  ],
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": "^16 || ^17 || ^18 || ^19",
    "react-dom": "^16 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react": "^4.1.0",
    "c8": "^8.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.js": "npm run lint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
